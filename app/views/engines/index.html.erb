<table>
  <tr>
    <th>Engine Name &nbsp; &nbsp;</th>
    <th>Last Run</th>
    <th>Users</th>
    <th>Connections</th>
  </tr>
    <% @engines.each do |engine| %>
  
  <tr>
    <td><%= link_to engine.engineName, engine_connections_path(engine.engineName) %></td>
    <td><%= engine.lastRun %></td>
    <td><%= engine.users %></td>
    <td><%= engine.connections %></td>
  </tr>

  <% end %>

</table>